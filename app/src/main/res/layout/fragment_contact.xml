<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.user.cn.Contact"
    android:background="#2345a1">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="left|top"
        android:background="#2345a1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/contact"
            android:id="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:textSize="30sp"
            android:textColor="#ffffff" />
        <android.support.v7.widget.ButtonBarLayout
            android:layout_width="2sp"
            android:layout_height="2sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:drawable/button_onoff_indicator_off"
            android:layout_marginLeft="-3dp"
            android:layout_marginRight="-3dp"
            android:id="@+id/view"></android.support.v7.widget.ButtonBarLayout>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            android:layout_below="@+id/view"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:background="#ffffff"
            android:height="95dp"
            android:layout_margin="0dp"
            android:textColor="#ffffff"
            android:backgroundTint="@android:color/transparent" />

        <android.support.v7.widget.ButtonBarLayout
            android:layout_width="2sp"
            android:layout_height="2sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:drawable/button_onoff_indicator_off"
            android:layout_marginTop="95dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginRight="-3dp"
            android:id="@+id/view1"></android.support.v7.widget.ButtonBarLayout>

        <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:id="@+id/imageView"
            android:layout_above="@+id/info"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="27dp"
            android:layout_marginStart="27dp"
            android:layout_below="@+id/view"
            android:background="@drawable/cont"
            android:backgroundTint="#ffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/names"
            android:id="@+id/info"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textIsSelectable="false"
            android:layout_alignBottom="@+id/button3"
            android:layout_alignRight="@+id/textView15"
            android:layout_alignEnd="@+id/textView15" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/som_thapa"
            android:id="@+id/textView10"
            android:textColor="#ffffff"
            android:layout_below="@+id/view"
            android:layout_alignRight="@+id/textView4"
            android:layout_alignEnd="@+id/textView4"
            android:layout_marginTop="35dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btncall"
            android:background="#ffffff"
            android:height="95dp"
            android:textColor="#ffffff"
            android:textSize="25sp"
            android:layout_below="@+id/view1"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:backgroundTint="@android:color/transparent" />
        <android.support.v7.widget.ButtonBarLayout
            android:layout_width="2sp"
            android:layout_height="2sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:drawable/button_onoff_indicator_off"
            android:layout_marginTop="190dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginRight="-3dp"
            android:id="@+id/view2"></android.support.v7.widget.ButtonBarLayout>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnemail"
            android:background="#ffffff"
            android:height="95dp"
            android:textColor="#ffffff"
            android:textSize="25dp"
            android:layout_below="@+id/view2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:backgroundTint="@android:color/transparent" />
        <android.support.v7.widget.ButtonBarLayout
            android:layout_width="2sp"
            android:layout_height="2sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:drawable/button_onoff_indicator_off"
            android:layout_marginTop="285dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginRight="-3dp"
            android:id="@+id/view3"></android.support.v7.widget.ButtonBarLayout>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnlocation"
            android:background="#ffffff"
            android:height="95dp"
            android:textColor="#ffffff"
            android:textSize="25dp"
            android:layout_below="@+id/view3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignRight="@+id/view3"
            android:layout_alignEnd="@+id/view3"
            android:backgroundTint="@android:color/transparent" />
        <android.support.v7.widget.ButtonBarLayout
            android:layout_width="2sp"
            android:layout_height="2sp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:drawable/button_onoff_indicator_off"
            android:layout_marginTop="380dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginRight="-3dp"
            android:id="@+id/view4"></android.support.v7.widget.ButtonBarLayout>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnweb"
            android:background="#ffffff"
            android:height="95dp"
            android:textColor="#ffffff"
            android:textSize="25dp"
            android:layout_below="@+id/view4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:backgroundTint="@android:color/transparent" />

        <ImageView
            android:layout_width="90dp"
            android:layout_height="65dp"
            android:id="@+id/imageView2"
            android:background="@drawable/phones"
            android:backgroundTint="#ffffff"
            android:layout_alignTop="@+id/btncall"
            android:layout_alignLeft="@+id/imageView"
            android:layout_alignStart="@+id/imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Call"
            android:id="@+id/textView11"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:layout_alignBottom="@+id/view2"
            android:layout_alignRight="@+id/textView13"
            android:layout_alignEnd="@+id/textView13" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="98460-83434"
            android:id="@+id/txt1"
            android:textColor="#ffffff"
            android:layout_alignBaseline="@+id/btncall"
            android:layout_alignBottom="@+id/btncall"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageView3"
            android:background="@android:drawable/ic_dialog_email"
            android:backgroundTint="#ffffff"
            android:layout_below="@+id/view2"
            android:layout_alignLeft="@+id/imageView2"
            android:layout_alignStart="@+id/imageView2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="E-mail"
            android:id="@+id/textView13"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/imageView3"
            android:layout_alignRight="@+id/imageView3"
            android:layout_alignEnd="@+id/imageView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="info@cntravels.com.np"
            android:id="@+id/textView14"
            android:textColor="#ffffff"
            android:layout_above="@+id/textView13"
            android:layout_alignLeft="@+id/txt1"
            android:layout_alignStart="@+id/txt1" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageView4"
            android:background="@drawable/location"
            android:backgroundTint="#ffffff"
            android:layout_below="@+id/view3"
            android:layout_alignLeft="@+id/imageView3"
            android:layout_alignStart="@+id/imageView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Location"
            android:id="@+id/textView15"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/imageView4"
            android:layout_alignLeft="@+id/imageView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/_28_213144"
            android:id="@+id/textView16"
            android:textColor="#ffffff"
            android:layout_above="@+id/textView17"
            android:layout_alignLeft="@+id/textView17"
            android:layout_alignStart="@+id/textView17" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="83.9587303"
            android:id="@+id/textView17"
            android:textColor="#ffffff"
            android:layout_above="@+id/textView15"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignStart="@+id/textView14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Website"
            android:id="@+id/textView18"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textIsSelectable="false"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/textView15"
            android:layout_alignStart="@+id/textView15" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageView5"
            android:backgroundTint="#ffffff"
            android:layout_above="@+id/textView18"
            android:layout_alignLeft="@+id/textView13"
            android:layout_alignStart="@+id/textView13"
            android:background="@drawable/web" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="www.cntravels.com"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:id="@+id/textView26"

            android:layout_alignBottom="@+id/imageView5"
            android:layout_alignLeft="@+id/textView17"
            android:layout_alignStart="@+id/textView17"
            android:layout_marginBottom="15dp" />

    </RelativeLayout>
</FrameLayout>
